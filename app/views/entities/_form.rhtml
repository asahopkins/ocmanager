<p><label for="entity_first_name">First name</label><br/>
<%= text_field 'entity', 'first_name'  %></p>

<p><label for="entity_middle_name">Middle name</label><br/>
<%= text_field 'entity', 'middle_name'  %></p>

<p><label for="entity_last_name">Last name</label><br/>
<%= text_field 'entity', 'last_name'  %></p>

<p><label for="entity_name_suffix">Name suffix</label><br/>
<%= text_field 'entity', 'name_suffix'  %></p>

<p><label for="entity_nickname">Nickname</label><br/>
<%= text_field 'entity', 'nickname'  %></p>

<p><label for="entity_voter_ID">Voter id</label><br/>
<%= text_field 'entity', 'voter_ID'  %></p>

<p><label for="entity_precinct">Precinct</label><br/>
<%= text_field 'entity', 'precinct'  %></p>

<p><label for="entity_languages">Languages</label><br/>
