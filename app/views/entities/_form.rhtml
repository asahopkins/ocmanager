<p><label for="entity_receive_phone">Receive phone</label><br/>
<select id="entity_receive_phone" name="entity[receive_phone]"><option value="false">False</option><option value="true">True</option></select></p>

<p><label for="entity_federal_ID">Federal id</label><br/>
<%= text_field 'entity', 'federal_ID'  %></p>

